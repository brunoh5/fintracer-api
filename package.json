{
  "name": "fintracer-api",
  "version": "1.0.0",
  "description": "Api de gestÃ£o financeira",
  "author": "Bruno Henrique",
  "license": "MIT",
  "homepage": "https://fintracer.com.br",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brunoh5/fintracer-api.git"
  },
  "engines": {
    "pnpm": ">=8",
    "node": ">=20"
  },
  "main": "index.js",
  "keywords": [],
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsup src --out-dir build",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "seed": "tsx prisma/seeds.ts",
    "start": "node build/server.js",
    "test:create-prisma-environment": "pnpm link --global --dir ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "pnpm link --global vitest-environment-prisma",
    "test": "vitest run --dir src/use-cases",
    "test:watch": "vitest --dir src/use-cases",
    "pretest:e2e": "npm-run-all test:create-prisma-environment test:install-prisma-environment",
    "pretest:coverage": "npm-run-all test:create-prisma-environment test:install-prisma-environment",
    "test:e2e": "vitest run --dir src/http",
    "test:e2e:watch": "vitest --dir src/http",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "1.10.0",
    "multer": "1.4.5-lts.1",
    "reflect-metadata": "^0.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^2.1.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/supertest": "6.0.0",
    "@vitest/coverage-v8": "1.1.0",
    "eslint": "^8.56.0",
    "npm-run-all": "4.1.5",
    "prisma": "^5.7.1",
    "supertest": "6.3.3",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite-tsconfig-paths": "^4.2.2",
    "vitest": "^1.1.0"
  }
}
